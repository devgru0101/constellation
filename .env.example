# Database Configuration
MONGODB_ROOT_PASSWORD=your_secure_mongodb_password_here
REDIS_PASSWORD=your_secure_redis_password_here

# Authentication
JWT_SECRET=your_jwt_secret_key_here_at_least_32_characters

# External Services
ANTHROPIC_API_KEY=your_anthropic_api_key_here
CLAUDE_API_KEY=your_claude_api_key_here

# Development Configuration
VITE_API_BASE_URL=http://localhost:8000
VITE_WS_URL=ws://localhost:8000/ws

# Container Configuration
DOCKER_REGISTRY_URL=
CONTAINER_TIMEOUT=300000

# Feature Flags
VITE_ENABLE_AI_FEATURES=true
VITE_ENABLE_CONTAINER_MANAGEMENT=true
VITE_ENABLE_GIT_INTEGRATION=true

# Development Settings
NODE_ENV=development
VITE_DEV_SERVER_PORT=3000

# Logging Configuration
LOG_LEVEL=info
ENABLE_DEBUG_LOGS=false

# Monitoring
GRAFANA_ADMIN_PASSWORD=your_grafana_admin_password_here

# Environment
COMPOSE_PROJECT_NAME=constellation

# SSL (for production)
ACME_EMAIL=your_email@domain.com
DOMAIN=constellation.yourdomain.com

# External Services (GitHub integration)
GITHUB_CLIENT_ID=your_github_client_id
GITHUB_CLIENT_SECRET=your_github_client_secret
